{
  "swagger": "2.0",
  "info": {
    "title": "Microsserviço Achamigos",
    "description": "Documentação do cadastro e autenticação de usuário",
    "version": "1.0.0"
  },
  "host": "localhost:5001",
  "schemes": [
    "http"
  ],
  "basePath": "/",
  "securityDefinitions": {
    "apiKeyAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "x-api-key"
    }
  },
  "paths": {
    "/users/register": {
      "post": {
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "nome": { "example": "any" },
                "cnpj": { "example": "any" },
                "telefone": { "example": "any" },
                "userLogin": { "example": "any" },
                "senha": { "example": "any" },
                "email": { "example": "any" },
                "endereco": { "example": "any" },
                "tipo": { "example": "any" },
                "userStatus": { "example": "any" },
                "linkUser": { "example": "any" }
              }
            }
          }
        ],
        "responses": {
          "201": { "description": "Created" },
          "400": { "description": "Bad Request" },
          "403": { "description": "Forbidden" },
          "500": { "description": "Internal Server Error" }
        }
      }
    },
    "/users/login": {
      "post": {
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "userLogin": { "example": "any" },
                "senha": { "example": "any" }
              }
            }
          }
        ],
        "responses": {
          "200": { "description": "OK" },
          "401": { "description": "Unauthorized" },
          "403": { "description": "Forbidden" },
          "500": { "description": "Internal Server Error" }
        }
      }
    }
  },
  "security": [
    { "apiKeyAuth": [] }
  ]
}
